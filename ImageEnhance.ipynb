    "from PIL import Image\n",
    "from PIL import ImageEnhance\n",
    "\n",
    "image = Image.open(\"C:/Users/Beyza/Desktop/image_processing/text2.jpg\")\n",
    "image.show()\n",
    "curr_sharp = ImageEnhance.Sharpness(image)\n",
    "new_sharp = 8.3\n",
    "img_sharped = curr_sharp.enhance(new_sharp)\n",
    "img_sharped.show()"
   ]
  }
